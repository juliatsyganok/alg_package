def quickSort(arr):
    if len(arr) <= 1:
        return arr
    else:
        q = arr[0]
        left = []
        ravn = []
        right = []
        for el in arr:
            if el < q:
                left.append(el) 
            elif el > q: 
                right.append(el) 
            else: 
                ravn.append(el)
        return left + ravn + right